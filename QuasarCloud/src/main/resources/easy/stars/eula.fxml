<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox id="window"
      prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="easy.stars.javafx.controllers.EULA">
   <CheckBox fx:id="agree" onAction="#eulaNoAccept" mnemonicParsing="false" selected="true" text="Я прочитал и принимаю лицензионное соглашение и политику компании"/>
   <CheckBox fx:id="log" mnemonicParsing="false" selected="true" text="Отправлять данные об ошибках на сервер"/>
   <Button fx:id="continueBtn" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#buttonClicked" text="Продолжить" textAlignment="CENTER"/>
   <Label fx:id="warning"/>
</VBox>
